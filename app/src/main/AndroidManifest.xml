<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="com.coalbrother.mobileshop">    <uses-permission android:name="android.permission.INTERNET" />    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />    <application        android:name=".common.MobileshopApp"        android:allowBackup="true"        android:icon="@mipmap/mobileshop_logo"        android:label="@string/app_name"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <activity android:name=".activity.SplashActivity">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity android:name=".activity.MainActivity" />        <activity android:name=".activity.AdActivity" />        <activity android:name=".activity.AnimationActivity" />        <activity            android:name=".activity.LoginActivity"            android:launchMode="singleTop">            <intent-filter>                <action android:name="com.coalbrother.mobileshop.LOGIN" />                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>        </activity>        <activity android:name=".view.TestActivity" />        <activity android:name=".activity.ChangePwdActivity" />        <activity android:name=".activity.MyAddressActivity" />        <activity android:name=".activity.MyCollectActivity" />        <activity android:name=".activity.MyOrderActivity" />        <activity android:name=".activity.GoodsListActivity" />        <activity android:name=".activity.GoodsDetailActivity"></activity>    </application></manifest>